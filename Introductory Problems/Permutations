#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin>>n;
    if(n==1){
        cout<<n<<endl;
    }
    else{
        if(n%2==0){
            if((n-1==1)){
                cout<<"NO SOLUTION"<<endl;
            }
            else{
                for(int i=2;i<=n;i+=2){
                    cout<<i<<" ";
                }
                for(int i=1;i<n;i+=2){
                    cout<<i<<" ";
                }
                cout<<endl;
            }
        }
        else{
            if((n-1)-1==1){
                cout<<"NO SOLUTION"<<endl;
            }
            else{
                for(int i=2;i<n;i+=2){
                    cout<<i<<" ";
                }
                for(int i=1;i<=n;i+=2){
                    cout<<i<<" ";
                }
                cout<<endl;
            }           
        }
    }
}
